: \"ForgotPassword\",\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      modalActive: false,\r\n      modalMessage: \"\",\r\n      loading: null,\r\n    };\r\n  },\r\n  components: {\r\n    email,\r\n    Modal,\r\n    Loading,\r\n  },\r\n  methods: {\r\n    resetPassword() {\r\n      this.loading = true;\r\n      firebase\r\n        .auth()\r\n        .sendPasswordResetEmail(this.email)\r\n        .then(() => {\r\n          this.modalMessage = \"If your account exists, you will receive a email\";\r\n          this.loading = false;\r\n          this.modalActive = true;\r\n        })\r\n        .catch((err) => {\r\n          this.modalMessage = err.message;\r\n          this.loading = false;\r\n          this.modalActive = true;\r\n        });\r\n    },\r\n    closeModal() {\r\n      this.modalActive = !this.modalActive;\r\n      //when error is closed empty the field\r\n      this.email = \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.reset-password {\r\n  position: relative;\r\n  .form-wrap {\r\n    .reset {\r\n      h2 {\r\n        margin-bottom: 8px;\r\n      }\r\n      p {\r\n        text-align: center;\r\n        margin-bottom: 32px;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}