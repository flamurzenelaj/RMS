ainer\">\r\n        <h2>\r\n          <i><b>Update Food</b></i>\r\n        </h2>\r\n        <div class=\"food-info\">\r\n          <div class=\"input\">\r\n            <div>\r\n              <label for=\"name\">Name:</label>\r\n              <input type=\"text\" id=\"name\" v-model=\"currentFood.name\" />\r\n            </div>\r\n            <div>\r\n              <label for=\"price\">Price:</label>\r\n              <input\r\n                type=\"number\"\r\n                id=\"price\"\r\n                v-model.number=\"currentFood.price\"\r\n                required\r\n              />\r\n            </div>\r\n            <div>\r\n              <label for=\"description\">Description:</label>\r\n              <textarea\r\n                placeholder=\"Enter your description here\"\r\n                type=\"text\"\r\n                v-model=\"currentFood.description\"\r\n              />\r\n            </div>\r\n          </div>\r\n          <button @click=\"updateFood\" class=\"button\">Update</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport apiRequest from \"../../util/apiRequest\";\r\n// import axios from \"axios\";\r\n\r\nexport default {\r\n  async mounted() {\r\n    this.routeID = this.$route.params.foodid;\r\n    this.currentFood = await apiRequest.getFood(this.$route.params.foodid);\r\n    //or\r\n    // await (await axios.get(\"http://localhost:3000/food/\"+this.$route.params.foodid)).data;\r\n  },\r\n  components: {},\r\n  data() {\r\n    return {\r\n      currentFood: null,\r\n      routeID:null\r\n    };\r\n  },\r\n  methods: {\r\n    async updateFood() {\r\n      //   const validationResult = FoodSchema.validate(this.form);\r\n      //   if (validationResult.error) {\r\n      //     this.error = error;\r\n      //     return;\r\n      //   }\r\n\r\n      await apiRequest.updateFood({ ...this.currentFood });\r\n      this.$router.push({ name: \"Home\" });\r\n    },\r\n  },\r\n};\r\n</script>\r\n